<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet author="david" id="create-motorcycle-table">
        <createTable tableName="motorcycle"
                     tablespace="A String">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="brand" type="varchar(150)"/>
            <column name="model" type="varchar(200)"/>
            <column name="color" type="varchar(100)"/>
            <column name="engine_capacity" type="varchar(20)"/>
            <column name="engine_power" type="varchar(20)"/>
            <column name="fabrication_year" type="varchar(20)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>